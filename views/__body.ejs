<div class="container-body">
    <div class="container-body-column">
        <div class="flex-column width-20pct">
            <div class="avatar flex-row anim-slide-in-top">
                <div class="avatar-pic">
                    <img src="./img/picto-aer.png" alt="">
                </div>
                <div class="avatar-profile flex-column">
                    <div class="profile flex-column">
                        <small>Nama</small>
                        <span><%=user.data.name%></span>
                    </div>
                    <div class="profile flex-column">
                        <small><%=(user.data.peringkat=='Rendah'?'Tahun':(user.data.peringkat=='Menengah'?'Tingkatan':''))%></small>
                        <span><%=user.data.darjah_tingkatan%></span>
                    </div>
                    <div class="profile flex-column">
                        <small>Sekolah</small>
                        <span><%=user.data.namasekolah%></span>
                    </div>
                    <div class="profile flex-column">
                        <small>Alamat</small>
                        <span><%=user.data.alamat%></span>
                    </div>
                </div>
            </div>
            <div class="menus flex-column anim-slide-in-top">
                <div class="menu-header">
                    Peringkat Negeri
                </div>
                <div class="menu-list flex-column">
                    <%
                    var theme = [
                        ['Aeroangkasa','aer'],
                        ['Kecerdasan Buatan dan Sains Data','kbs'],
                        ['Inovasi, Kreativiti untuk Kesejahteraan Rakyat','ikr'],
                        ['Teknologi Sohor','ts'],
                        ['Keterangkuman Malaysia Madani','ktm'],
                        ['Sains & Inovasi','sci']                    
                    ];

                    var getLogo = (th)=>{
                        //console.log('SKAKJHKJAHKHAKJS: ',th,theme.filter(a=>a[0]==th));
                        return theme.filter(a=>a[0]==th)[0][1];
                    }
                    %>
                    <%competitions.filter(a=>a.peringkat==='Negeri').forEach(d=>{%>
                        <div class="menu-item flex-row">
                            <img src="./img/picto-<%=getLogo(d.theme)%>.png" alt="">
                            <%=d.prog_name%>
                        </div>
                    <%})%>
                </div>

                <div class="menu-header">
                    Peringkat Sekolah
                </div>
                <div class="menu-list flex-column">
                    
                    <%competitions.filter(a=>a.peringkat==='Sekolah').forEach(d=>{%>
                        <div class="menu-item flex-row">
                            <img src="./img/picto-<%=getLogo(d.theme)%>.png" alt="">
                            <%=d.prog_name%>
                        </div>
                    <%})%>
                </div>
            </div>
        </div>
        <div class="main-container anim-slide-in-top flex-column width-50pct flex-grow-2  scroll">
            <div class="book shadow flex-column">
                <div class="book-header flex-row">
                    Teknologi Sohor | 5.3K Techlympics Quiz-A-Thon
                </div>
                <div class="book-container">
                    <div class="book-list flex-row">
                        <div class="tarikh">
                            05-07-2023
                        </div>
                        <div class="book-title">
                            Soalan Quiz Sekolah Rendah Set 1
                        </div>
                        <div class="status float-right">
                            00:15:32
                        </div>
                    </div>
                    <div class="book-list flex-row">
                        <div class="tarikh">
                            06-07-2023
                        </div>
                        <div class="book-title">
                            Soalan Quiz Sekolah Rendah Set 2
                        </div>
                        <div class="status float-right">
                            Belum
                        </div>
                    </div>
                    <div class="book-list flex-row">
                        <div class="tarikh">
                            07-07-2023
                        </div>
                        <div class="book-title">
                            Soalan Quiz Sekolah Rendah Set 3
                        </div>
                        <div class="status float-right">
                            Belum
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="flex-column width-20pct">
            <div class="youtube">
                <iframe width="100%" height="315" src="https://www.youtube.com/embed/AGKhWREcl-Q" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
            <ul class="tree">
                <li>
                    Rujukan
                    <ul class="tree">
                        <%theme.forEach(d=>{%>
                        <li class="li-item"><img src="./img/picto-<%=d[1]%>.png" alt=""><%=d[0]%></li>    
                        <%})%>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    
</div>